{
    "name": "slam/phpactor-extension",
    "type": "phpactor-extension",
    "description": "PhpActor extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "microsoft/tolerant-php-parser": "^0.0.23",
        "phpactor/container": "^2.0.0",
        "phpactor/map-resolver": "^1.4.0",
        "phpactor/worse-reflection": "^0.4.5",
        "phpactor/worse-reflection-extension": "^0.2.4"
    },
    "require-dev": {
        "malukenho/mcbumpface": "^1.1.5",
        "phpactor/test-utils": "^1.1.3",
        "phpstan/phpstan": "^0.12.90",
        "phpstan/phpstan-phpunit": "^0.12.20",
        "phpunit/phpunit": "^9.5.6",
        "psr/container": "^1.1.1",
        "slam/php-cs-fixer-extensions": "^3.0.1"
    },
    "extra": {
        "mc-bumpface": {
            "stripVersionPrefixes": true
        },
        "phpactor.extension_class": "SlamPhpactor\\Extension\\SlamExtension"
    },
    "autoload": {
        "psr-4": {
            "SlamPhpactor\\Extension\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamPhpactor\\Extension\\Tests\\": "tests/"
        }
    }
}
